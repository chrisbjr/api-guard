{
    "name": "chrisbjr/api-guard",
    "description": "A simple way of authenticating your APIs with API keys using Laravel",
    "keywords": [
        "api",
        "rest",
        "json",
        "api keys",
        "api authentication",
        "laravel"
    ],
    "homepage": "https://github.com/chrisbjr/ApiGuard",
    "authors": [
        {
            "name": "Chris Bautista",
            "email": "chrisbjr@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.2.*",
        "illuminate/database": "*",
        "league/fractal": "0.9.*",
        "ellipsesynergie/api-response": "0.7.*"
    },
    "autoload": {
        "classmap": [
            "src/migrations",
            "src/controllers",
            "src/models"
        ],
        "psr-0": {
            "Chrisbjr\\ApiGuard\\": "src/"
        }
    },
    "minimum-stability": "stable"
}
